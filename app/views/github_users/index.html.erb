<p id="notice"><%= notice %></p>

<h1>Listing Github Users</h1>

<table>
  <thead>
    <tr>
      <th>Github login</th>
      <th>Github</th>
      <th>Avatar url</th>
      <th>Url</th>
      <th>Html url</th>
      <th>Type</th>
      <th>Name</th>
      <th>Company</th>
      <th>Email</th>
      <th>Public repos</th>
      <th>Github created at</th>
      <th>Github updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @github_users.each do |github_user| %>
      <tr>
        <td><%= github_user.github_login %></td>
        <td><%= github_user.github_id %></td>
        <td><%= github_user.avatar_url %></td>
        <td><%= github_user.url %></td>
        <td><%= github_user.html_url %></td>
        <td><%= github_user.type %></td>
        <td><%= github_user.name %></td>
        <td><%= github_user.company %></td>
        <td><%= github_user.email %></td>
        <td><%= github_user.public_repos %></td>
        <td><%= github_user.github_created_at %></td>
        <td><%= github_user.github_updated_at %></td>
        <td><%= link_to 'Show', github_user %></td>
        <td><%= link_to 'Edit', edit_github_user_path(github_user) %></td>
        <td><%= link_to 'Destroy', github_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Github user', new_github_user_path %>
